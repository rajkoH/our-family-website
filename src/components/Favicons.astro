---
import appleTouchIcon from '~/assets/favicons/apple-touch-icon.png';
const cacheBust = Date.now();
---

<link rel="icon" type="image/svg+xml" href={`/our-family-website/favicon.svg?v=${cacheBust}`} />
<link rel="shortcut icon" href={`/our-family-website/favicon.svg?v=${cacheBust}`} />
<link rel="mask-icon" href={`/our-family-website/favicon.svg?v=${cacheBust}`} color="#D08C64" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
